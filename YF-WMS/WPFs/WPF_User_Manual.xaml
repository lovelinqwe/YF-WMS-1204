<Window x:Class="YF_WMS.WPFs.WPF_User_Manual"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YF_WMS.WPFs"
        mc:Ignorable="d"
        Title="用户添加" Height="300" Width="300">
    <Grid>
        <Label Content="用户名：" HorizontalAlignment="Left" Margin="60,55,0,0" VerticalAlignment="Top"/>
        <Label Content="密码：" HorizontalAlignment="Left" Margin="60,107,0,0" VerticalAlignment="Top"/>
        <TextBox Name="username" HorizontalAlignment="Left" Height="23" Margin="137,55,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <PasswordBox Name="password" HorizontalAlignment="Left" Margin="137,113,0,0" VerticalAlignment="Top" Width="120"/>
        <Button Content="确认" HorizontalAlignment="Left" Margin="50,199,0,0" VerticalAlignment="Top" Width="75" Click="user_manual_determine_Click"/>
        <Button Content="退出" HorizontalAlignment="Left" Margin="172,199,0,0" VerticalAlignment="Top" Width="75" Click="user_manual_cancel_Click"/>

    </Grid>
</Window>
