<Window x:Class="YF_WMS.WPF_Role"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="权限管理-角色管理" Height="500" Width="600">
    <Grid>
        <Button Content="添加" HorizontalAlignment="Left" Margin="19,10,0,0" VerticalAlignment="Top" Width="75" Click="Role_Add"/>
        <Button Content="菜单-编辑" HorizontalAlignment="Left" Margin="19,92,0,0" VerticalAlignment="Top" Width="75" Click="Role_Edit"/>
        <Button Content="删除" HorizontalAlignment="Left" Margin="19,37,0,0" VerticalAlignment="Top" Width="75" Click="Role_Delete"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="19,64,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="查找" HorizontalAlignment="Left" Margin="144,65,0,0" VerticalAlignment="Top" Width="75" Click="Role_Select"/>
        <Button Content="导出" HorizontalAlignment="Left" Margin="19,158,0,0" VerticalAlignment="Top" Width="75" Click="Role_Export"/>
        <DataGrid HorizontalAlignment="Left" Height="255" Margin="10,205,0,0" VerticalAlignment="Top" Width="572"/>

    </Grid>
</Window>
